{
  "author": "Michael Hueter <mthueter@gmail.com>",
  "version": "2.0.0",
  "name": "docker-node-express-boilerplate",
  "description": "Boilerplate Dockerized Express code to launch new RESTful APIs.",
  "repository": {
    "type": "git",
    "url": "git@github.com:mhueter/docker-node-express-boilerplate.git"
  },
  "main": "app/app.js",
  "scripts": {
    "test": "jest",
    "dev": "env-cmd -f .env npx nodemon app/server.js",
    "staging": "env-cmd -f .env.staging npx nodemon app/server.js",
    "start": "env-cmd -f .env.staging node app/server.js"
  },
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bluebird": "3.7.2",
    "env-cmd": "^10.1.0",
    "express": "4.17.1",
    "jsonschema": "1.4.0",
    "mongoose": "^5.12.7",
    "nodemon": "^2.0.12",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-preset-es2017": "^6.24.1",
    "eslint": "^7.26.0",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "mongodb-memory-server": "^6.9.6",
    "supertest": "^6.1.3"
  },
  "jest": {
    "globalSetup": "./config/jest/setup.js",
    "globalTeardown": "./config/jest/teardown.js",
    "testEnvironment": "./config/jest/MongoEnvironment.js"
  }
}
